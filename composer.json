{
    "name": "eserozvataf/scabbia2-yaml",
    "description": "Scabbia2 Yaml Component",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    },
    "keywords": [
        "eserozvataf",
        "scabbia",
        "scabbia2",
        "scabbiafw",
        "yaml",
        "parser"
    ],
    "homepage": "https://github.com/eserozvataf/scabbia2",
    "license": [
        "Apache-2.0",
        "MIT"
    ],
    "authors": [
        {
            "name": "Eser Ozvataf",
            "homepage": "http://eser.ozvataf.com/"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/eserozvataf/scabbia2-yaml/blob/master/contributors.md"
        },
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/eserozvataf/scabbia2-yaml/issues",
        "source": "https://github.com/eserozvataf/scabbia2-yaml"
    },
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "eserozvataf/scabbia2-tasks": "*",
        "eserozvataf/scabbia2-testing": "*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Scabbia\\Yaml\\": [ "src/Yaml/" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scabbia\\Tests\\Yaml\\": [ "tests/Yaml/" ]
        }
    }
}
